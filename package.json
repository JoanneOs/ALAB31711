{
  "compilerOptions": {
    "strict": true,
    "noEmitOnError": true,
    "module": "commonjs",
    "jsx": "preserve",
    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": true,
    "lib": [
      "es6",
      "dom"
    ],
    "rootDir": "tsSetup",  
    "outDir": "out",       
    "moduleResolution": "node"
  },
  "include": [
    "tsSetup/**/*"        
}